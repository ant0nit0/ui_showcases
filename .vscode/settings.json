{
  "editor.formatOnSave": true,
  "dart.flutterSdkPath": ".fvm/versions/3.35.4",
  "dart.sdkPath": ".fvm/flutter_sdk/bin/cache/dart-sdk",
  "[dart]": {
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.organizeImports": "always"
    }
  },
  "actionButtons": {
    "commands": [
      {
        "name": "pub get",
        "singleInstance": true,
        "command": "fvm flutter pub get"
      },
      {
        "name": "build_runner watch",
        "singleInstance": true,
        "command": "fvm dart run build_runner watch --delete-conflicting-outputs"
      },
      {
        "name": "slang",
        "singleInstance": true,
        "command": "fvm dart run slang"
      }
    ]
  },
  "java.configuration.updateBuildConfiguration": "disabled"
}